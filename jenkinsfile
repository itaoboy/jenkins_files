pipeline {
    agent any

     stages {
        stage('Build') {
            steps {
	        def username='jenkins_name'
                echo 'output---:${username}'
		ech0 'output global--${env.JENKINS_URL}'
		echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
